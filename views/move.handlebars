<html>
{{#each foo}}
{{this}}
{{/each}}
<h3>Add a Move:</h3>
<form id="addmove" action="/move" method="post">
  Move Name: <input type="text" name="move_name"><br>
  <input type="submit" value="Submit">
</form>
<br />

<h3>Current Moves:</h3>
<table>
  <thead>
    <th>Move ID</th>
    <th>Move Name</th>
  </thead>
  <tbody>
    {{#each move}}
    <tr>
      <td>{{move_id}}</td>
      <td>{{move_name}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

</html>