<html>
{{#each foo}}
{{this}}
{{/each}}
<h3>Add a Location:</h3>
<form id="addlocation" action="/location" method="post">
  Location Name: <input type="text" name="loc_name"><br>
  Gym?  <input type="text" name="loc_gym"><br>
  Pokemon Center? <input type="text" name="loc_pc"><br>
  <input type="submit" value="Submit">
</form>
<br />

<h3>Current Locations:</h3>
<table>
  <thead>
    <th>Location ID</th>
    <th>Location Name</th>
    <th>Gym?</th>
    <th>Pokemon Center?</th>
  </thead>
  <tbody>
    {{#each location}}
    <tr>
      <td>{{loc_id}}</td>
      <td>{{loc_name}}</td>
      <td>{{loc_gym}}</td>
      <td>{{loc_pc}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

</html>